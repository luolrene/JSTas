import axios from '@/utils/axios'

// 客户信息查询
export function getCrmAccountsReceivableCustQueryPageData (params) {
  return new Promise((resolve, reject) => {
    axios.post(process.env.JS_Server + '/CrmAccountsReceivable/cust/queryPageData', params).then(res => {
      resolve(res.data)
    }).catch(err => {
      reject(err)
    })
  })
}
