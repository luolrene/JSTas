import axios from '@/utils/axios'

// 合同-指标信息查询
export function getContTaskQueryList (params) {
  return new Promise((resolve, reject) => {
    axios.post(process.env.JS_Server + '/contTask/queryList', params).then(res => {
      resolve(res.data)
    }).catch(err => {
      reject(err)
    })
  })
}
